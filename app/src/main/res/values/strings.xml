    <string name="id_intro">
        #include&lt;stdio.h&gt;\n
        int main(){\n
            int i,a=54,b=24,gcd;\n
            int minValue=(a&lt;b)?a:b;\n
            for(i=1;i&lt;=minValue;i++){\n
                if(a%i==0 &amp;&amp; b%i==0){\n
                    gcd=i;\n
                }\n
            }\n
            printf("Your GCD Is %d",gcd);\n
        }\n
    </string>
